{
  "name": "sistema-otica-unificado",
  "version": "1.0.0",
  "description": "Sistema Unificado de Gestão de Ótica - Agendamentos + Controle Financeiro",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:fast": "vite --host --force",
    "dev:optimized": "node start-dev.js",
    "dev:clean": "node start-dev.js --clean",
    "build": "tsc && vite build",
    "build:watch": "tsc --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "restart": "npm run clean && npm run dev",
    "kill-port": "npx kill-port 3002",
    "start": "node iniciar-limpo.cjs",
    "start:clean": "node limpar-e-iniciar.cjs",
    "kill-all": "node -e \"require('child_process').execSync('for /f \\\"tokens=5\\\" %a in (\\\"netstat -ano ^| findstr :3000 :3001 :3002 :3003 :3004 :3005 :5173 :4173\\\") do taskkill /f /pid %a 2>nul', {stdio: 'inherit'})\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@mui/x-date-pickers": "^6.18.0",
    "@supabase/supabase-js": "^2.39.0",
    "@types/styled-components": "^5.1.34",
    "chart.js": "^4.4.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-datepicker": "^4.24.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.20.0",
    "react-toastify": "^9.1.3",
    "recharts": "^2.8.0",
    "styled-components": "^6.1.19",
    "xlsx": "^0.18.5",
    "yup": "^1.4.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-datepicker": "^4.19.0",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "keywords": [
    "react",
    "typescript",
    "supabase",
    "material-ui",
    "otica",
    "agendamento",
    "financeiro",
    "gestao"
  ],
  "author": "Sistema de Gestão de Ótica",
  "license": "MIT"
}
